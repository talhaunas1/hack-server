{
    "version": 2,
    "env": {
      "DATABASE_URL": "@database-url",
      "SECRET_KEY": "@secret-key"
    },
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  